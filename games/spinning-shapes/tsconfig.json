{
    "compilerOptions": {
        "module": "none",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "removeComments": false,
        "target": "es5",
        "sourceMap": true,
        "rootDir": ".",
        "outFile": "game.js",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "declaration": true
    },
    "exclude": [
        "node_modules",
        "out",
        "test/_workspace"
    ]
}